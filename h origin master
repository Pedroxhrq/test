[36m@@ -0,0 +1,44 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Http\Middleware;[m
[32m+[m
[32m+[m[32muse Closure;[m
[32m+[m[32muse Illuminate\Contracts\Auth\Factory as Auth;[m
[32m+[m
[32m+[m[32mclass Authenticate[m
[32m+[m[32m{[m
[32m+[m[32m    /**[m
[32m+[m[32m     * The authentication guard factory instance.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @var \Illuminate\Contracts\Auth\Factory[m
[32m+[m[32m     */[m
[32m+[m[32m    protected $auth;[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Create a new middleware instance.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param  \Illuminate\Contracts\Auth\Factory  $auth[m
[32m+[m[32m     * @return void[m
[32m+[m[32m     */[m
[32m+[m[32m    public function __construct(Auth $auth)[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->auth = $auth;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Handle an incoming request.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param  \Illuminate\Http\Request  $request[m
[32m+[m[32m     * @param  \Closure  $next[m
[32m+[m[32m     * @param  string|null  $guard[m
[32m+[m[32m     * @return mixed[m
[32m+[m[32m     */[m
[32m+[m[32m    public function handle($request, Closure $next, $guard = null)[m
[32m+[m[32m    {[m
[32m+[m[32m        if ($this->auth->guard($guard)->guest()) {[m
[32m+[m[32m            return response('Unauthorized.', 401);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $next($request);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m

[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Http\Controllers;[m
[32m+[m
[32m+[m[32mclass CalculoController extends Controller[m
[32m+[m
[32m+[m[32m{[m
[32m+[m	[32mpublic function imc($peso, $altura)[m
[32m+[m	[32m{[m
[32m+[m		[32mreturn ($peso / ($altura * $altura) * 10000);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mprivate function delta($a, $b, $c)[m
[32m+[m	[32m{[m
[32m+[m		[32mreturn (($b * $b) - ((4 * $a) * $c));[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mprivate function erro($mensagem)[m
[32m+[m	[32m{[m
[32m+[m		[32mreturn response()->json([m
[32m+[m[32m            [[m
[32m+[m[32m                'status' => "erro",[m
[32m+[m[32m                'mensagem' => $mensagem[m
[32m+[m[32m            ][m
[32m+[m[32m        );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mpublic function bhaskara($a, $b, $c)[m
[32m+[m	[32m{[m
[32m+[m		[32mif ($a == 0)[m
[32m+[m		[32m{[m
[32m+[m			[32mreturn $this->erro("O valor de A nao pode ser igual a zero!");[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m$delta = $this->delta($a, $b, $c);[m
[32m+[m		[32mif ($delta < 0)[m
[32m+[m		[32m{[m
[32m+[m			[32mreturn $this->erro("O valor de Delta nao pode ser negativo!");[m
[32m+[m		[32m}[m
[32m+[m		[32melse[m
[32m+[m		[32m{[m
[32m+[m			[32m$x1 = (-$b + sqrt($delta)) / (2 * $a);[m
[32m+[m			[32m$x2 = (-$b - sqrt($delta)) / (2 * $a);[m
[32m+[m			[32mreturn response()->json([m
[32m+[m[32m                [[m
[32m+[m[32m                    'x1' => $x1,[m
[32m+[m[32m                    'x2' => $x2[m
[32m+[m[32m                ][m
[32m+[m[32m            );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
\ No newline at end of file[m
[1mdiff --git a/app/Http/Controllers/CalculoController_01.php b/app/Http/Controllers/CalculoController_01.php[m
[1mnew file mode 100644[m
[1mindex 0000000..9a0b7bf[m
[1m--- /dev/null[m
[1m+++ b/app/Http/Controllers/CalculoController_01.php[m
[36m@@ -0,0 +1,54 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Http\Controllers;[m
[32m+[m
[32m+[m[32mclass CalculoController extends Controller[m
[32m+[m[32m{[m
[32m+[m[32m    public function imc($peso, $altura)[m
[32m+[m[32m    {[m
[32m+[m[32m        return ($peso / ($altura * $altura)*10000);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    private function delta($a, $b, $c)[m
[32m+[m[32m    {[m
[32m+[m[32m        return (($b*$b)-((4*$a)*$c));[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    private function erro($mensagem)[m
[32m+[m[32m    {[m[41m [m
[32m+[m[32m        return response() -> json([m
[32m+[m[32m            [[m
[32m+[m[32m                'status' => "erro",[m
[32m+[m[32m                'mensagem' => $mensagem[m
[32m+[m[32m            ][m
[32m+[m[32m        );[m
[32m+[m[41m            [m
[32m+[m[32m    }[m
[32m+[m[41m        [m
[32m+[m[32m    public function bascara($a, $b, $c)[m
[32m+[m[32m    {[m
[32m+[m[32m    if ($a == 0)[m
[32m+[m[32m    {[m
[32m+[m[32m        return $this -> erro("Valor de A deve ser diferente 0!");[m
[32m+[m[32m    }[m
[32m+[m[32m    $delta = $this->delta($a,$b,$c);[m
[32m+[m[32m    if ($delta < 0)[m
[32m+[m[32m    {[m
[32m+[m[32m        return $this -> erro("Delta negativo!");[m
[32m+[m[32m    }[m
[32m+[m[32m    else[m
[32m+[m[32m    {[m
[32m+[m[32m        $x1 = (-$b + sqrt($delta)) / (2*$a);[m
[32m+[m[32m        $x2 = (-$b - sqrt($delta)) / (2*$a);[m
[32m+[m[32m        return response() -> json([m
[32m+[m[32m            [[m
[32m+[m[32m                'x1' => $x1,[m
[32m+[m[32m                'x2' => $x2[m
[32m+[m[32m                ][m
[32m+[m[32m            );[m
[32m+[m[32m        }[m[41m   [m
[32m+[m[41m        [m
[32m+[m[32m    }[m
[32m+[m[41m            [m
[32m+[m[41m            [m
[32m+[m[32m    }[m
\ No newline at end of file[m

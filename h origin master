[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Providers;[m
[32m+[m
[32m+[m[32muse App\User;[m
[32m+[m[32muse Illuminate\Support\Facades\Gate;[m
[32m+[m[32muse Illuminate\Support\ServiceProvider;[m
[32m+[m
[32m+[m[32mclass AuthServiceProvider extends ServiceProvider[m
[32m+[m[32m{[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Register any application services.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return void[m
[32m+[m[32m     */[m
[32m+[m[32m    public function register()[m
[32m+[m[32m    {[m
[32m+[m[32m        //[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Boot the authentication services for the application.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return void[m
[32m+[m[32m     */[m
[32m+[m[32m    public function boot()[m
[32m+[m[32m    {[m
[32m+[m[32m        // Here you may define how you wish users to be authenticated for your Lumen[m
[32m+[m[32m        // application. The callback which receives the incoming request instance[m
[32m+[m[32m        // should return either a User instance or null. You're free to obtain[m
[32m+[m[32m        // the User instance via an API token or any other method necessary.[m
[32m+[m
[32m+[m[32m        $this->app['auth']->viaRequest('api', function ($request) {[m
[32m+[m[32m            if ($request->input('api_token')) {[m
[32m+[m[32m                return User::where('api_token', $request->input('api_token'))->first();[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m[32m}[m

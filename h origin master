[32m+[m
[32m+[m[32m    public function listarPorId($id)[m
[32m+[m[32m    {[m
[32m+[m[32m        return $this-> perguntas[$id]["respostas"];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function listarTodasPerguntas()[m
[32m+[m[32m    {[m
[32m+[m[32m        $apenasPerguntas = [];[m
[32m+[m[32m        $cont = 1;[m
[32m+[m[32m        foreach ($this-> perguntas as $pergunta) {[m
[32m+[m[32m            $apenasPerguntas[$cont] = $pergunta["enunciado"];[m
[32m+[m[32m            $cont++;[m
[32m+[m[32m        }[m
[32m+[m[32m        return $apenasPerguntas;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function responder($idPergunta, $idResposta)[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->respostas[$idPergunta] = $idResposta;[m
[32m+[m[32m        return $this->respostas;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function responderTodas($arrayPerguntas, $arrayRespostas)[m
[32m+[m[32m    {[m
[32m+[m[32m        $perguntas = explode("," , $arrayPerguntas);[m
[32m+[m[32m        $respostasLocais = explode("," , $arrayRespostas);[m
[32m+[m[32m        $cont = 0;[m
[32m+[m[32m        foreach($perguntas as $a) {[m
[32m+[m[32m            $this->responder($perguntas[$cont], $respostasLocais[$cont]);[m
[32m+[m[32m            $cont++;[m
[32m+[m[32m        }[m
[32m+[m[32m        return $this->respostas;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function calcular($arrayPerguntas, $arrayRespostas)[m
[32m+[m[32m    {[m
[32m+[m[32m        $perguntas = explode("," , $arrayPerguntas);[m
[32m+[m[32m        $respostas = explode("," , $arrayRespostas);[m
[32m+[m[41m        [m
[32m+[m[32m        if (count($respostas) != count($perguntas)) {[m
[32m+[m[32m            return response()->json([m
[32m+[m[32m                [[m
[32m+[m[32m                'Erro' => 'O numero de respostas nao equivale ao numero de perguntas!'[m
[32m+[m[32m                ][m
[32m+[m[32m            );[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $respostas = $this -> responderTodas ($arrayPerguntas, $arrayRespostas);[m
[32m+[m[32m        $resultado = 0;[m
[32m+[m
[32m+[m[32m        foreach ($perguntas as $pergunta){[m
[32m+[m[32m            $resultado = $resultado + $respostas[$pergunta];[m
[32m+[m[32m        }[m
[32m+[m[32m        if ($resultado >= 0 && $resultado <=7) {[m
[32m+[m[32m            return response()->json([m
[32m+[m[32m                [[m
[32m+[m[32m                'Resultado' => $resultado,[m
[32m+[m[32m                'Descricao' => 'Seu resultado indica consumo de bebida alcoolica de baixo risco ou abstemio.'[m
[32m+[m[32m                ][m
[32m+[m[32m            );[m
[32m+[m[32m        } elseif ($resultado >= 8 && $resultado <=15) {[m
[32m+[m[32m            return response()->json([m
[32m+[m[32m                [[m
[32m+[m[32m                'Resultado' => $resultado,[m
[32m+[m[32m                'Descricao' => 'Seu resultado indica consumo de bebida alcoolica de risco.'[m
[32m+[m[32m                ][m
[32m+[m[32m            );[m
[32m+[m[32m        } elseif ($resultado >= 16 && $resultado <=15) {[m
[32m+[m[32m            return response()->json([m
[32m+[m[32m                [[m
[32m+[m[32m                'Resultado' => $resultado,[m
[32m+[m[32m                'Descricao' => 'Seu resultado indica consumo de bebida alcoolica de uso nocivo ou de alto risco.'[m
[32m+[m[32m                ][m
[32m+[m[32m            );[m
[32m+[m[32m        } elseif ($resultado >= 17 && $resultado <=40) {[m
[32m+[m[32m            return response()->json([m
[32m+[m[32m                [[m
[32m+[m[32m                'Resultado' => $resultado,[m
[32m+[m[32m                'Descricao' => 'Seu resultado indica consumo de bebida alcoolica de provavel dependencia.'[m
[32m+[m[32m                ][m
[32m+[m[32m            );[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
